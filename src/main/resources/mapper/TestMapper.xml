<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iljesus.zceblog.mapper.TestMapper">

    <resultMap id="BaseResultMap" type="com.iljesus.zceblog.entity.Test">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="city" column="city" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,city
    </sql>
    <select id="findAll" resultType="com.iljesus.zceblog.entity.Test">
        select * from test;
    </select>
    <select id="findCitybyid" resultType="java.lang.String">
        select city from test where id = #{id}
    </select>
</mapper>
